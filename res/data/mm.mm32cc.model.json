{
    "name": "MM32CC Embedded Toolchain",
    "id": "MM32CC",
    "toolPrefix": "",
    "useUnixPath": true,
    "version": {
        "args": "--version",
        "matcher": "\\((?<name>.*?)\\) (?<version>[\\d\\.]+)"
    },
    "global": {
        "microcontroller-cpu": {
            "type": "keyValue",
            "command": "-mcpu=",
            "enum": {
                "cortex-m0": "cortex-m0",
                "cortex-m0+": "cortex-m0plus",
                "cortex-m23": "cortex-m23",
                "cortex-m3": "cortex-m3",
                "cortex-m33": "cortex-m33",
                "cortex-m33-none": "cortex-m33",
                "cortex-m33-sp": "cortex-m33",
                "cortex-m4-none": "cortex-m4",
                "cortex-m4-sp": "cortex-m4",
                "cortex-m7-none": "cortex-m7",
                "cortex-m7-sp": "cortex-m7",
                "cortex-m7-dp": "cortex-m7"
            },
            "group": [
                "c",
                "cpp",
                "asm",
                "linker"
            ],
            "location": "first"
        },
        "microcontroller-fpu": {
            "type": "selectable",
            "command": {
                "cortex-m0": "",
                "cortex-m0+": "",
                "cortex-m23": "",
                "cortex-m3": "",
                "cortex-m33": "",
                "cortex-m33-none": "",
                "cortex-m33-sp": "-mfpu=fpv5-sp-d16",
                "cortex-m4-none": "",
                "cortex-m4-sp": "-mfpu=fpv4-sp-d16",
                "cortex-m7-none": "",
                "cortex-m7-sp": "-mfpu=fpv5-sp-d16",
                "cortex-m7-dp": "-mfpu=fpv5-d16"
            },
            "group": [
                "c",
                "cpp",
                "asm",
                "linker"
            ],
            "location": "first"
        },
        "$linker-config": {
            "type":"selectable",
            "command": {
                "cortex-m0": "armv6m_soft_nofp_nosys",
                "cortex-m0+": "unknown",
                "cortex-m23": "unknown",
                "cortex-m3": "armv7m_soft_nofp_nosys",
                "cortex-m33": "unknown",
                "cortex-m33-none": "unknown",
                "cortex-m33-sp": "unknown",
                "cortex-m4-none": "armv7em_soft_nofp_nosys",
                "cortex-m4-sp": "armv7em_hard_fpv4_sp_d16_nosys",
                "cortex-m7-none": "unknown",
                "cortex-m7-sp": "unknown",
                "cortex-m7-dp": "unknown"
            },
            "group": [
                "linker"
            ]
        },
        "$float-abi-type": {
            "type": "selectable",
            "command": {
                "softfp": "softfp",
                "hard": "hard",
                "false": "softfp"
            },
            "group": [
                "c",
                "cpp",
                "asm",
                "linker"
            ]
        },
        "microcontroller-float": {
            "type": "selectable",
            "command": {
                "cortex-m0": "",
                "cortex-m0+": "",
                "cortex-m23": "",
                "cortex-m3": "",
                "cortex-m33": "-mfloat-abi=soft",
                "cortex-m33-none": "-mfloat-abi=soft",
                "cortex-m33-sp": "-mfloat-abi=${$float-abi-type}",
                "cortex-m4-none": "-mfloat-abi=soft",
                "cortex-m4-sp": "-mfloat-abi=${$float-abi-type}",
                "cortex-m7-none": "-mfloat-abi=soft",
                "cortex-m7-sp": "-mfloat-abi=${$float-abi-type}",
                "cortex-m7-dp": "-mfloat-abi=${$float-abi-type}"
            },
            "group": [
                "c",
                "cpp",
                "asm",
                "linker"
            ],
            "location": "first"
        },
        "output-debug-info": {
            "type": "selectable",
            "command": {
                "enable": "-g",
                "disable": "",
                "dwarf-2": "-g -gdwarf-2",
                "dwarf-3": "-g -gdwarf-3",
                "false": ""
            },
            "group": [
                "c",
                "cpp",
                "asm"
            ]
        },
        "C_FLAGS": {
            "type": "list",
            "command": "",
            "group": [
                "c"
            ]
        },
        "CXX_FLAGS": {
            "type": "list",
            "command": "",
            "group": [
                "cpp"
            ]
        },
        "ASM_FLAGS": {
            "type": "list",
            "command": "",
            "group": [
                "asm"
            ]
        },
        "LD_FLAGS": {
            "type": "list",
            "command": "",
            "group": [
                "linker"
            ]
        },
        "misc-control": {
            "type": "list",
            "command": "",
            "group": [
                "c",
                "cpp",
                "asm",
                "linker"
            ]
        },
        "one-elf-section-per-function": {
            "type": "selectable",
            "command": {
                "true": "-ffunction-sections",
                "false": ""
            },
            "group": [
                "c",
                "cpp"
            ]
        },
        "one-elf-section-per-data": {
            "type": "selectable",
            "command": {
                "true": "-fdata-sections",
                "false": ""
            },
            "group": [
                "c",
                "cpp"
            ]
        },
        "remove-unused-input-sections": {
            "type": "selectable",
            "command": {
                "true": "-Wl,--gc-sections",
                "false": ""
            },
            "group": [
                "linker"
            ]
        }
    },
    "groups": {
        "c": {
            "$path": "bin\\mm32cc.exe",
            "$includes": {
                "body": "-I${value}"
            },
            "$defines": {
                "body": "-D${key}=\"${value}\""
            },
            "$invoke": {
                "useFile": true,
                "body": "@${value}"
            },
            "$output": "-o ${out} -MMD ${in}",
            "$default": [
                "-c",
                "-xc",
                "-mthumb"
            ],
            "$language-c": {
                "type": "keyValue",
                "command": "-std=",
                "enum": {
                    "default": "c11",
                    "c89": "c89",
                    "c90": "c90",
                    "c99": "c99",
                    "c11": "c11",
                    "c1x": "c1x",
                    "c17": "c17",
                    "gnu89": "gnu89",
                    "gnu90": "gnu90",
                    "gnu99": "gnu99",
                    "gnu11": "gnu11",
                    "gnu1x": "gnu1x",
                    "gnu17": "gnu17"
                }
            },
            "optimization": {
                "type": "selectable",
                "command": {
                    "false": "",
                    "level-0": "-O0",
                    "level-1": "-O1",
                    "level-2": "-O2",
                    "level-3": "-O3",
                    "level-size": "-Os",
                    "level-fast": "-Ofast",
                    "level-debug": "-Og"
                }
            },
            "turn-Warning-into-errors": {
                "type": "selectable",
                "command": {
                    "true": "-Werror",
                    "false": ""
                }
            },
            "warnings": {
                "type": "selectable",
                "command": {
                    "false": "",
                    "no-warnings": "-w",
                    "all-warnings": "-Wall"
                }
            }
        },
        "cpp": {
            "$path": "bin\\mm32cc.exe",
            "$includes": {
                "body": "-I${value}"
            },
            "$defines": {
                "body": "-D${key}=\"${value}\""
            },
            "$invoke": {
                "useFile": true,
                "body": "@${value}"
            },
            "$output": "-o ${out} -MMD ${in}",
            "$default": [
                "-c",
                "-xc++",
                "-mthumb"
            ],
            "$language-cpp": {
                "type": "keyValue",
                "command": "-std=",
                "enum": {
                    "default": "c++11",
                    "c++98": "c++98",
                    "gnu++98": "gnu++98",
                    "c++11": "c++11",
                    "gnu++11": "gnu++11",
                    "c++14": "c++14",
                    "gnu++14": "gnu++14",
                    "c++17": "c++17",
                    "gnu++17": "gnu++17"
                }
            },
            "optimization": {
                "type": "selectable",
                "command": {
                    "false": "-O0",
                    "level-0": "-O0",
                    "level-1": "-O1",
                    "level-2": "-O2",
                    "level-3": "-O3",
                    "level-size": "-Os",
                    "level-fast": "-Ofast",
                    "level-debug": "-Og"
                }
            },
            "turn-Warning-into-errors": {
                "type": "selectable",
                "command": {
                    "true": "-Werror",
                    "false": ""
                }
            },
            "warnings": {
                "type": "selectable",
                "command": {
                    "false": "",
                    "no-warnings": "-w",
                    "all-warnings": "-Wall"
                }
            }
        },
        "asm": {
            "$path": "bin\\mm32cc.exe",
            "$includes": {
                "body": "-I${value}"
            },
            "$output": "-o ${out} -MMD ${in}",
            "$invoke": {
                "useFile": true,
                "body": "@${value}"
            },
            "$default": [
                "-c",
                "-x assembler-with-cpp",
                "-mthumb"
            ],
            "defines": {
                "type": "list",
                "command": "-D"
            }
        },
        "linker": {
            "$path": "bin\\mm32cc.exe",
            "$libs": {
                "body": "-L${value}"
            },
            "$invoke": {
                "useFile": true,
                "body": "@${value}"
            },
            "$default": [
                "-mthumb --config ${$linker-config}"
            ],
            "$default-tail": [
            ],
            "$output": "-o ${out} ${in} ${lib_flags}",
            "$outputSuffix": ".elf",
            "$linkMap": {
                "type": "value",
                "command": "-Wl,-Map=${mapPath}"
            },
            "$LIB_FLAGS": {
                "type": "list",
                "command": ""
            },
            "link-scatter": {
                "type": "list",
                "command": "-T "
            },
            "$outputBin": [
                {
                    "name": "output hex file",
                    "toolPath": "bin\\llvm-objcopy.exe",
                    "outputSuffix": ".hex",
                    "command": "-O ihex ${linkerOutput} ${output}"
                },
                {
                    "name": "output bin file",
                    "toolPath": "bin\\llvm-objcopy.exe",
                    "outputSuffix": ".bin",
                    "command": "-O binary ${linkerOutput} ${output}"
                }
            ]
        },
        "linker-lib": {
            "$path": "bin\\llvm-ar.exe",
            "$invoke": {
                "useFile": true,
                "body": "@${value}"
            },
            "$output": "-rcv ${out} ${in}",
            "$outputSuffix": ".a"
        }
    }
}